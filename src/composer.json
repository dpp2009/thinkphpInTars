    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "5.1.*",
        "topthink/think-swoole": "^2.0",
        "phptars/tars-registry": "~0.1.0",
        "phptars/tars-deploy": "~0.1.0",
        "phptars/tars-utils": "~0.1.0",
        "phptars/tars-report": "~0.1.0",
        "phptars/tars-client": "~0.1.0",
        "phptars/tars2php": "~0.1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
